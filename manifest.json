{
    "name": "Getting Started Example",
    "version": "1.0",
    "description": "Build an Extension!",
    "permissions": ["storage"],
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "browser_action": {
      "default_popup": "/panel/panel.html",
      "default title": "Tool tip test"
    },
    "icons": {
        "16": "images/ying-yang.svg",
        "32": "images/ying-yang.svg",
        "48": "images/ying-yang.svg",
        "128": "images/ying-yang.svg"
    },
    "manifest_version": 2
  }